{
  "name": "frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "json-summary": "jest --coverage --coverageReporters json-summary",
    "coverage-report": "jest --coverage --reporters=default --reporters=jest-junit | tee ./coverage.txt && exit $(PIPESTATUS[0])",
    "snapshot:update": "jest --updateSnapshot",
    "schema:update": "npx openapi-typescript http://127.0.0.1:8000/openapi.json  --output 'src/data/schema.ts'",
    "cucumber": "cucumber-js --require cucumber.js --require /features/*.features --require /features/step_definitions/*.js"
    },
    "bin": {
      "cucumber-js": "bin/cucumber.js"
    },
  "dependencies": {
    "@chakra-ui/icons": "^2.0.18",
    "@chakra-ui/react": "^2.5.5",
    "@cucumber/cucumber": "^9.1.1",
    "@next/font": "13.1.6",
    "@types/node": "18.14.0",
    "@types/node-fetch": "^2.6.2",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "core-js": "^3.29.1",
    "eslint-config-next": "13.1.6",
    "eslint-plugin-unused-imports": "^2.0.0",
    "igniteui-angular": "^15.1.1",
    "isomorphic-fetch": "^3.0.0",
    "jest-junit": "^15.0.0",
    "next": "13.1.6",
    "node-fetch": "^3.3.1",
    "react": "18.2.0",
    "react-cookie": "^4.1.1",
    "react-dom": "18.2.0",
    "react-icons": "^4.8.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/isomorphic-fetch": "^0.0.36",
    "@typescript-eslint/eslint-plugin": "^5.53.0",
    "@typescript-eslint/parser": "^5.53.0",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-react": "^7.32.2",
    "jest": "^29.4.3",
    "jest-environment-jsdom": "^29.4.3",
    "next-router-mock": "^0.9.2",
    "typescript": "^4.9.5"
  }
}
