{
    "extends": "next/core-web-vitals",
    "plugins": ["@typescript-eslint", "unused-imports"],
    "rules": {
        "indent": [
            "warn",
            "tab"
        ],
        "linebreak-style": [
            "warn",
            "windows"
        ],
        "quotes": [
            "warn",
            "double"
        ],
        "@typescript-eslint/no-unused-vars": 1,
        "unused-imports/no-unused-imports-ts": 1
    }
}